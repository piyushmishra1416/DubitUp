{
   "manifest_version": 3,
   "name": "YouTube Video Time Extractor",
   "version": "1.0",
   "description": "Extracts the current playing time of a YouTube video.",
   "permissions": ["activeTab", "scripting", "storage", "tabs"],
   "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
   "action": {
     "default_popup": "index.html"
   }
 }
 