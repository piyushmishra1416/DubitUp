{
   "manifest_version": 3,
   "name": "YouTube Video Time Extractor",
   "version": "1.0",
   "description": "Extracts the current playing time of a YouTube video.",
   "permissions": ["activeTab", "scripting"],
   "background": {
     "service_worker": "src/background.ts"
   },
   "action": {
     "default_popup": "index.html"
   },
   "content_scripts": [
     {
       "matches": ["https://www.youtube.com/*"],
       "js": ["src/content.ts"]
     }
   ]
 }
 