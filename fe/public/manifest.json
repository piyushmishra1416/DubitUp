{
   "manifest_version": 3,
   "name": "YouTube Video Time Extractor",
   "version": "1.0",
   "description": "Extracts the current playing time of a YouTube video.",
   "permissions": ["activeTab", "scripting"],
   "background": {
     "service_worker": "background.ts"
   },
   "action": {
     "default_popup": "index.html",
     "default_icon": {
       "16": "images/icon16.png",
       "48": "images/icon48.png",
       "128": "images/icon128.png"
     }
   },
   "icons": {
     "16": "images/icon16.png",
     "48": "images/icon48.png",
     "128": "images/icon128.png"
   },
   "content_scripts": [
     {
       "matches": ["https://www.youtube.com/*"],
       "js": ["content.ts"]
     }
   ]
 }
 